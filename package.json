{
  "name": "vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "bun run typecheck && biome lint --fix",
    "format": "biome format --write",
    "check": "biome check --fix",
    "preview": "vite preview",
    "prepare": "husky",
    "api:generate": "rimraf ./src/api/generated && npx openapi-generator-cli generate"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@solana-program/token": "^0.7.0",
    "@solana/kit": "^4.0.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "@tailwindcss/vite": "4.1.14",
    "@tanstack/react-query": "^5.90.2",
    "@types/big.js": "^6.2.2",
    "axios": "^1.12.2",
    "big.js": "^7.0.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.545.0",
    "next-themes": "^0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.4",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.14",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-svgr": "^4.5.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@openapitools/openapi-generator-cli": "^2.25.0",
    "@types/node": "24.7.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react-swc": "4.1.0",
    "husky": "9.1.7",
    "lightningcss": "1.30.2",
    "lint-staged": "16.2.4",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vite-tsconfig-paths": "5.1.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md}": [
      "bash -c 'files=\"$@\"; files=$(echo \"$files\" | tr \" \" \"\\n\" | grep -v \"src/api/generated\" | tr \"\\n\" \" \"); [ -n \"$files\" ] && biome check --fix --files-ignore-unknown=true $files || true' --"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
