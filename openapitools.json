{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.11.0",
    "generators": {
      "local": {
        "inputSpec": "https://apistaging.backyard.finance/openapi",
        "generatorName": "typescript-axios",
        "output": "./src/api/generated",
        "additionalProperties": {
          "useSingleRequestParameter": true,
          "enumPropertyNaming": "UPPERCASE",
          "withSeparateModelsAndApi": true,
          "apiPackage": "api",
          "modelPackage": "types"
        }
      },
      "jupiter-swap": {
        "inputSpec": "https://raw.githubusercontent.com/jup-ag/docs/refs/heads/main/openapi-spec/ultra/ultra.yaml",
        "generatorName": "typescript-axios",
        "output": "./src/jupiter/api/generated/swap",
        "additionalProperties": {
          "useSingleRequestParameter": true,
          "enumPropertyNaming": "UPPERCASE",
          "withSeparateModelsAndApi": true,
          "apiPackage": "api",
          "modelPackage": "types"
        }
      }
    }
  }
}
